% g, git

;; aliases

# Git :: g
git 

# Git Status :: gs
git status


;; commands

# Initialize a git repository
git init

# Change Branch :: gsw
git switch <branch>

# Git Clone :: gcl
git clone <git_repo>


% ghq

# GHQ Clone :: gcl
ghq get <git_repo>

$ branch: git branch | awk '{print $NF}'
$ git_repo: echo git@github.com/<username>/<repo>.git
