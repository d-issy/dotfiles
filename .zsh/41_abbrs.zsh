abbrs=(
    # system
    e   'export'
    m   'make'
    mc  'make -C'
    mn  'make -n'
    mnc 'make -n -C'
    t   'touch'
    md  'mkdir'

    # git
    g    'git'
    ga   'git add'
    gb   'git branch'
    gc   'git checkout'
    gca  'git commit -ammend,n'
    gcb  'git checkout -b'
    gco  'git commit,n'
    gd   'git diff'
    gf   'git fetch'
    gl   'git log'
    gs   'git status,n'
    gpl 'git pull'
    gps 'git push'

    'git branch ' 'git branch -a'
    'git branch ' 'git branch -a'
    'git pull '   'git pull origin'
    'git push '   'git push origin'
    'git fetch '  'git fetch origin'

    # docker
    d      'docker'
    dc     'docker-compose'
    dps    'docker ps'
    dr     'docker run'
    dstart 'docker start'
    dstop  'docker stop'

    # tar
    tar 'tar xzvf'

    # sudo
    s 'sudo'

    # tig
    ts  'tig status'

    # npm
    nid 'npm install --save-dev'
    nis 'npm install --save'

    # editor
    v 'vim,f'
    i 'idea .,n'
    c 'code .,n'
)
